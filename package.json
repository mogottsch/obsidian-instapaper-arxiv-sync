{
  "name": "obsidian-instapaper-arxiv-sync",
  "version": "0.1.0",
  "description": "Sync ArXiv papers from Instapaper to Obsidian",
  "main": "main.js",
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "node esbuild.config.mjs --watch",
    "build": "node esbuild.config.mjs --production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "pnpm typecheck && pnpm lint && pnpm format:check && pnpm test",
    "prepare": "husky"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "obsidian-plugin",
    "instapaper",
    "arxiv",
    "papers",
    "research"
  ],
  "author": "Moritz Gottschling <mogottsch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mogottsch/obsidian-instapaper-arxiv-sync"
  },
  "bugs": {
    "url": "https://github.com/mogottsch/obsidian-instapaper-arxiv-sync/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.4",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.4"
  }
}
